<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" 
      content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>First Express</title>
</head>
<body>
        <h1>All To Do</h1>
        <ul>
          <% todos.forEach(function(t) { %>
            <li>
              <%= t.todo %>
                - 
              <%= t.done ? 'done' : 'not done' %>
            </li>
          <% }); %>
        </ul>
</body>
</html>
